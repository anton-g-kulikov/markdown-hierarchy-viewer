{
  "name": "markdown-ascii-tree",
  "displayName": "Markdown ASCII Tree",
  "description": "Live ASCII tree preview for Markdown headers with real-time updates and proper text wrapping.",
  "version": "1.0.0",
  "publisher": "your-publisher-name",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/markdown-ascii-tree"
  },
  "keywords": [
    "markdown",
    "tree",
    "outline",
    "headers",
    "structure",
    "ascii",
    "preview"
  ],
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdown-ascii-tree.showAsciiTree",
        "title": "Markdown: Show ASCII Header Tree",
        "when": "editorLangId == markdown"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "markdown-ascii-tree.showAsciiTree",
          "when": "resourceExtname == .md",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
